<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.TextArea?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="Cabinet.controller.ModifierAnamneseController"
      prefWidth="300" prefHeight="200" alignment="CENTER">

    <Label text="Modifier Anamnèse" style="-fx-font-size: 18px; -fx-font-weight: bold;"/>

    <Label text="Nouvelle Catégorie:"/>
    <TextField fx:id="categorieField"/>

    <Label text="Nouvel Énoncé:"/>
    <TextArea fx:id="enonceField"/>

    <Button text="Enregistrer" onAction="#handleEnregistrer"/>
</VBox>
